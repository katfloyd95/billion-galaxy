 <h1>{{section.settings.heading}}</h1> {% comment %} title {% endcomment %}

<h3>{{section.settings.question}}</h3> {% comment %} question {% endcomment %}

<p>{{section.settings.answer}}</p> {% comment %} answer {% endcomment %}

{% schema %}
{
  "name": "customer questions",
  "settings": [
    {
      "type": "text",
      "label": "heading for faq",
      "id": "heading"
    },
    {
      "type": "text",
      "label": "customer questions",
      "id": "question"
    },
    {
      "type": "textarea",
      "label": "answer for question",
      "id": "answer"
    }
  ]
}
{% endschema %}